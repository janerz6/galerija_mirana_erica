            <div class="col-md-6 col-sm-6 wback">
                <div class="col-md-10 col-md-offset-1 col-xs-10 col-xs-offset-1 interviewContent">
                    <br>
                    {{#each data.parts}}
                        <h5 id="{{@key}}">{{this.title}}</h5>
                        <br/>
                        <p>{{{this.content}}}</p>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
